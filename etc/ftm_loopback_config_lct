tc 1
#Headset-mic to Speaker Dual-Channel AFE loopback
!AfeLoop
Txdevice:5
Rxdevice:5

enable
SLIM RX0 MUX:AIF_MIX1_PB
SLIM_0_RX Channels:One
RX INT6_2 MUX:RX0
COMP6 Switch:1
RX0 Digital Volume:100

AIF1_CAP Mixer SLIM TX6:1
SLIM_0_TX Channels:One
SLIM TX6 MUX:DEC6
ADC MUX6:AMIC
AMIC MUX6:ADC2
IIR0 INP0 MUX:DEC6
ADC2 Volume:23
DEC6 Volume:84

SLIMBUS_DL_HL Switch:1
SLIMBUS_0_RX Port Mixer SLIM_0_TX:1

disable
SLIMBUS_0_RX Port Mixer SLIM_0_TX:0
SLIMBUS_DL_HL Switch:0

DEC6 Volume:84
ADC1 Volume:12
IIR0 INP0 MUX:ZERO
AMIC MUX6:ZERO
ADC MUX6:AMIC
SLIM TX6 MUX:ZERO
AIF1_CAP Mixer SLIM TX6:0

COMP6 Switch:0
RX INT6_2 MUX:ZERO
SLIM RX0 MUX:ZERO

tc 2
#Headset-mic to Receiver AFE loopback
!AfeLoop
Txdevice:5
Rxdevice:5

enable
SLIM RX0 MUX:AIF_MIX1_PB
SLIM_0_RX Channels:One
RX INT0_1 MIX1 INP0:RX0
RX INT0 DEM MUX:CLSH_DSM_OUT
EAR PA Gain:G_6_DB

AIF1_CAP Mixer SLIM TX6:1
SLIM_0_TX Channels:One
SLIM TX6 MUX:DEC6
ADC MUX6:AMIC
AMIC MUX6:ADC2
IIR0 INP0 MUX:DEC6
ADC2 Volume:12
DEC6 Volume:84

SLIMBUS_DL_HL Switch:1
SLIMBUS_0_RX Port Mixer SLIM_0_TX:1

disable
SLIMBUS_0_RX Port Mixer SLIM_0_TX:0
SLIMBUS_DL_HL Switch:0

DEC6 Volume:0
ADC1 Volume:0
IIR0 INP0 MUX:ZERO
AMIC MUX6:ZERO
ADC MUX6:ZERO
SLIM TX6 MUX:ZERO
AIF1_CAP Mixer SLIM TX6:0

RX INT0_1 MIX1 INP0:ZERO
SLIM RX0 MUX:ZERO

tc 3
#Handset-mic to Headset Dual-Channel AFE loopback
!AfeLoop
Txdevice:5
Rxdevice:5

enable
SLIM RX0 MUX:AIF_MIX1_PB
SLIM RX1 MUX:AIF_MIX1_PB
SLIM_0_RX Channels:Two
RX INT1_2 MUX:RX0
RX INT2_2 MUX:RX1
RX INT1 DEM MUX:CLSH_DSM_OUT
RX INT2 DEM MUX:CLSH_DSM_OUT
HPHL Volume:100
HPHR Volume:100
RX0 Digital Volume:68
RX1 Digital Volume:68

AIF1_CAP Mixer SLIM TX6:1
SLIM_0_TX Channels:One
SLIM TX6 MUX:DEC6
ADC MUX6:AMIC
AMIC MUX6:ADC1
IIR0 INP0 MUX:DEC6
ADC1 Volume:12
DEC6 Volume:84

SLIMBUS_DL_HL Switch:1
SLIMBUS_0_RX Port Mixer SLIM_0_TX:1

disable
SLIMBUS_0_RX Port Mixer SLIM_0_TX:0
SLIMBUS_DL_HL Switch:0

DEC6 Volume:84
ADC1 Volume:12
IIR0 INP0 MUX:ZERO
AMIC MUX6:ZERO
ADC MUX6:AMIC
SLIM TX6 MUX:ZERO
AIF1_CAP Mixer SLIM TX6:0

RX1 Digital Volume:0
RX0 Digital Volume:0
HPHR Volume:0
HPHL Volume:0
RX INT2_2 MUX:ZERO
RX INT1_2 MUX:ZERO
SLIM_0_RX Channels:One
SLIM RX1 MUX:ZERO
SLIM RX0 MUX:ZERO

