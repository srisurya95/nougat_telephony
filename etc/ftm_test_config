tc 1
#Handset Capture
!Capture
Txdevice:0

enable
MultiMedia1 Mixer SLIM_0_TX:1
AIF1_CAP Mixer SLIM TX6:1
SLIM_0_TX Channels:One
SLIM TX6 MUX:DEC6
ADC MUX6:AMIC
AMIC MUX6:ADC1
IIR0 INP0 MUX:DEC6
ADC1 Volume:12
DEC6 Volume:84

disable
DEC6 Volume:84
ADC1 Volume:12
IIR0 INP0 MUX:ZERO
AMIC MUX6:ZERO
ADC MUX6:AMIC
SLIM TX6 MUX:ZERO
AIF1_CAP Mixer SLIM TX6:0
MultiMedia1 Mixer SLIM_0_TX:0

tc 2
#Ear Piece
!Playback
Rxdevice:0

enable
SLIMBUS_0_RX Audio Mixer MultiMedia1:1
SLIM RX0 MUX:AIF_MIX1_PB
SLIM_0_RX Channels:One
RX INT0_1 MIX1 INP0:RX0
RX INT0 DEM MUX:CLSH_DSM_OUT
EAR PA Gain:G_6_DB

disable
RX INT0_1 MIX1 INP0:ZERO
SLIM RX0 MUX:0
SLIMBUS_0_RX Audio Mixer MultiMedia1:0

